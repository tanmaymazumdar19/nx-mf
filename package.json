    "lint:org": "yarn nx lint org",
    "lint:poc": "yarn nx lint poc",
    "lint:team": "yarn nx lint team",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "format": "prettier ./apps/**/src/** ./apps/**/*.ts ./libs/**/src/** --write",
    "lint-staged": "lint-staged"
